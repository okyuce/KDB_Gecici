@{
    ViewBag.Title = "Giriş";
    Layout = "~/Areas/Admin/Views/Shared/_EmptyLayout.cshtml";
}
<div class="d-flex flex-column flex-root" id="kt_app_root">
    <div class="d-flex flex-column flex-lg-row flex-column-fluid">
        <div class="d-flex flex-lg-row-fluid">
            <div class="d-flex flex-column flex-center pb-0 pb-lg-10 p-10 w-100">
                <img class="theme-light-show mx-auto mw-100 w-150px w-lg-300px mb-10 mb-lg-20" src="~/img/logo2.png" alt="" />
                <img class="theme-dark-show mx-auto mw-100 w-150px w-lg-300px mb-10 mb-lg-20" src="~/admin/metronic/assets/media/auth/agency-dark.png" alt="" />
                <h1 class="text-gray-800 fs-2qx fw-bold text-center mb-7">Yönetim Paneli</h1>
            </div>
        </div>
        <div class="d-flex flex-column-fluid flex-lg-row-auto justify-content-center justify-content-lg-end p-12">
            <div class="bg-body shadow-sm d-flex flex-column flex-center rounded-4 w-md-600px p-10">
                <div class="d-flex flex-center flex-column align-items-stretch h-lg-100 w-md-400px">
                    <div class="d-flex flex-center flex-column flex-column-fluid pb-15 pb-lg-20">
                        <form class="form w-100" novalidate="novalidate" id="form_giris">
                            <div class="text-center mb-11">
                                <h1 class="text-dark fw-bolder mb-3">Giriş Yap</h1>
                            </div>
                            <div class="fv-row mb-2">
                                <input type="text" placeholder="Kullanıcı Adı" name="KullaniciAdi" autocomplete="off" class="form-control bg-transparent" />
                            </div>
                            <div class="fv-row mb-2">
                                <input type="password" placeholder="Şifre" name="Sifre" autocomplete="off" class="form-control bg-transparent" />
                            </div>
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="fv-row mb-2">
                                        <img id="imgGuvenlikKodu" src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==" />
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="fv-row mb-2">
                                        <input type="text" placeholder="Güvenlik Kodu" name="guvenlikKodu" autocomplete="off" class="form-control bg-transparent" />
                                    </div>
                                </div>
                            </div>
                            <div class="d-grid mb-2">
                                <a href="javascript:SifremiUnuttumModal()">
                                    <i class="fa fa-lock"></i> Şifremi Unuttum
                                </a>
                            </div>
                            <div class="d-grid mb-10">
                                <button id="btn_giris" class="btn btn-primary">
                                    <span class="indicator-label">Giriş Yap</span>
                                </button>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="modal fade" id="modal_sifremi_unuttum" tabindex="-1" aria-hidden="true">
    <div class="modal-dialog modal modal-dialog-centered">
        <div class="modal-content rounded">
            <div class="modal-header pb-0 border-0 justify-content-end">
            </div>
            <div class="modal-body scroll-y px-10 px-lg-10 pt-0 pb-10">
                <form id="modal_sifremi_unuttum_form" onsubmit="return false;" novalidate="novalidate" class="form">
                    <div class="mb-5 text-center">
                        <h1 class="mb-3 modal-title">Şifremi Unuttum</h1>
                    </div>
                    <div class="alert alert-primary">Buradan <b>bakanlık personeli <u>olmayan</u></b> kişiler şifre sıfırlama yapabilmektedir.</div>
                    <div class="row">
                        <div class="col-md-12">
                            <div class="d-flex flex-column mb-8 fv-row">
                                <label class="d-flex align-items-center fs-6 fw-semibold mb-2">
                                    <span class="required">Kullanıcı Adı</span>
                                </label>
                                <input type="text" data-validate="required" data-title="Kullanıcı Adı" class="form-control form-control-solid" placeholder="Kullanıcı Adı" name="KullaniciAdi" />
                            </div>
                        </div>
                        <div class="col-md-12">
                            <div class="d-flex flex-column mb-8 fv-row">
                                <label class="d-flex align-items-center fs-6 fw-semibold mb-2">
                                    <span class="required">TC Kimlik No</span>
                                </label>
                                <input type="text" data-validate="required" data-title="TC Kimlik No" class="form-control form-control-solid identity-mask" autocomplete="off" placeholder="TC Kimlik No" name="TCKimlikNo" />
                            </div>
                        </div>
                    </div>
                    <div class="d-flex flex-column mb-8 fv-row">
                        <label class="d-flex align-items-center fs-6 fw-semibold mb-2">
                            <span id="spEposta" class="required">E-Posta</span>
                        </label>
                        <input type="text" class="form-control form-control-solid email-mask" data-validate="required" placeholder="E-posta" name="EPosta" />
                    </div>

                    <div class="row mb-5">
                        <div class="col-md-5">
                            <div class="fv-row mb-2">
                                <img id="imgSifremiUnuttumGuvenlikKodu" src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==" />
                            </div>
                        </div>
                        <div class="col-md-7">
                            <div class="fv-row mb-2">
                                <input type="text" placeholder="Güvenlik Kodu" name="guvenlikKodu" autocomplete="off" class="form-control bg-transparent" />
                            </div>
                        </div>
                    </div>
                    <div class="text-center">
                        <button type="reset" id="modal_sifremi_unuttum_iptal" class="btn btn-light me-3">İptal</button>
                        <button type="submit" id="modal_sifremi_unuttum_kaydet" class="btn btn-primary">Şifremi Sıfırla</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="modal_dogrulama_kodu" tabindex="-1" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered">
        <div class="modal-content rounded">
            <div class="modal-header pb-0 border-0 justify-content-end">
            </div>
            <div class="modal-body scroll-y px-10 px-lg-10 pt-0 pb-10">
                <form id="modal_dogrulama_kodu_form" onsubmit="return false;" novalidate="novalidate" class="form">
                    <div class="mb-5 text-center">
                        <h1 class="mb-3 modal-title">Doğrulama Kodu</h1>
                    </div>
                    <div class="alert alert-primary">Telefonunuza gönderilen kod ile sisteme giriş yapabilirsiniz.</div>
                    <div class="row">
                        <div class="col-md-12">
                            <div class="d-flex flex-column mb-8 fv-row">
                                <input type="text" data-validate="required" data-title="Doğrulama Kodu" class="form-control form-control-solid" placeholder="Doğrulama Kodu" autocomplete="off" name="DogrulamaKodu" />
                            </div>
                        </div>
                    </div>
                    <div class="text-center">
                        <button type="button" class="btn btn-light me-3" data-bs-dismiss="modal">İptal</button>
                        <button type="submit" class="btn btn-primary" id="modal_dogrulama_kodu_giris">Giriş</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>

@section Scripts {
    <script type="text/javascript">
        let guvenlikKoduYenilemePeriyodu = 60000 * 4;
        let girisGuid = "";

        $(document).ready(function () {
            guvenlikKoduYukle();
            window.guvenlikKoduYukleInterval = setInterval(guvenlikKoduYukle, guvenlikKoduYenilemePeriyodu);

            $('#modal_sifremi_unuttum').on('hidden.bs.modal', function (event) {
                $("#modal_sifremi_unuttum input").val("").trigger('change');
            });

            $('#modal_sifremi_unuttum').on('shown.bs.modal', function (event) {
                sifremiUnuttumGuvenlikKoduYukle();
            });
            $(document).on('click', "#modal_sifremi_unuttum_iptal", function (e) {
                $("#modal_sifremi_unuttum").modal("hide");
            });

            $(document).on('click', "#modal_sifremi_unuttum_kaydet", function (e) {
                e.preventDefault();

                if (!validateForm("#modal_sifremi_unuttum_form")) {
                    return;
                }

                let formData = serializeFormAsObject("#modal_sifremi_unuttum_form");

                formData["guvenlikKoduKey"] = $("#imgSifremiUnuttumGuvenlikKodu").attr("data-key");

                startLoader($("#modal_sifremi_unuttum"));
                Request("/admin/kullanici/sifremiUnuttum", formData, 'post')
                    .done(function (response) {
                        stopLoader($("#modal_sifremi_unuttum"));

                        toastr.success("Yeni şifreniz mail adresinize gönderilmiştir.");

                        $("#modal_sifremi_unuttum").modal("hide");
                    })
                    .fail(function (jqXHR, textStatus) {
                        sifremiUnuttumGuvenlikKoduYukle();
                        stopLoader($("#modal_sifremi_unuttum"));
                        toastr.error(getAjaxErrorMessage(jqXHR));
                    });
            });


            $(document).on('submit', "#form_giris", function (e) {
                e.preventDefault();
                clearInterval(window.guvenlikKoduYukleInterval);

                let formData = serializeFormAsObject("#form_giris");

                formData["guvenlikKoduKey"] = $("#imgGuvenlikKodu").attr("data-key");

                startLoader("#kt_app_root");

                RequestText("/admin/giris", formData, 'post')
                    .done(function (response) {
                        resetForm($("#modal_dogrulama_kodu_form"));
                        stopLoader("#kt_app_root");
                        girisGuid = response;
                        $("#modal_dogrulama_kodu").modal("show");
                    }).fail(function (jqXHR, textStatus) {
                        stopLoader("#kt_app_root");
                        toastr.error(getAjaxErrorMessage(jqXHR));
                        guvenlikKoduYukle();
                        window.guvenlikKoduYukleInterval = setInterval(guvenlikKoduYukle, guvenlikKoduYenilemePeriyodu);
                    });
            });

            $(document).on('submit', "#modal_dogrulama_kodu_form", function (e) {
                e.preventDefault();                
                let formData = serializeFormAsObject("#modal_dogrulama_kodu_form");
                formData["girisGuid"] = girisGuid;
                startLoader("#kt_app_root");
                RequestText("/admin/girisdogrula", formData, 'post')
                    .done(function (response) {
                        toastr.success("Başarıyla giriş yapıldı, yönlendiriliyorsunuz..");

                        location.href = "/admin/";
                    }).fail(function (jqXHR, textStatus) {
                        stopLoader("#kt_app_root");
                        toastr.error(getAjaxErrorMessage(jqXHR));
                        guvenlikKoduYukle();
                        window.guvenlikKoduYukleInterval = setInterval(guvenlikKoduYukle, guvenlikKoduYenilemePeriyodu);
                    });
            });

            $(document).on('click', "#btn_giris", function (e) {
                e.preventDefault();

                $("#form_giris").submit();
            });
        });

        function SifremiUnuttumModal() {
            $("#modal_sifremi_unuttum").modal("show");
            return;
        }

        function guvenlikKoduYukle() {
            $("#imgGuvenlikKodu").attr("src", "data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==").attr("data-key", "");

            Request("/admin/getirGuvenlikKodu", null, 'post')
                .done(function (response) {
                    $("#imgGuvenlikKodu").attr("src", "data:image/jpeg;base64," + response.base64).attr("data-key", response.key);
                }).fail(function (jqXHR, textStatus) {
                    $("#imgGuvenlikKodu").attr("src", 'data:image/png;base64,');
                    toastr.error("Güvenlik kodu oluşturulurken hata oluştu!");
                });
        }

        function sifremiUnuttumGuvenlikKoduYukle() {
            $("#imgSifremiUnuttumGuvenlikKodu").attr("src", "data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==").attr("data-key", "");

            Request("/admin/getirGuvenlikKodu", null, 'post')
                .done(function (response) {
                    $("#imgSifremiUnuttumGuvenlikKodu").attr("src", "data:image/jpeg;base64," + response.base64).attr("data-key", response.key);
                }).fail(function (jqXHR, textStatus) {
                    $("#imgSifremiUnuttumGuvenlikKodu").attr("src", 'data:image/png;base64,');
                    toastr.error("Güvenlik kodu oluşturulurken hata oluştu!");
                });
        }

    </script>
}