                return;
            }

            //startLoader("[data-name='tableList']");
            Request("/admin/basvurudosya/basvurudosyaindir", { basvuruDosyaId: basvuruDosyaId, tcKimlikNo: tcKimlikNo }, 'post')
                .done(function (response) {
                    let a = document.createElement("a");
