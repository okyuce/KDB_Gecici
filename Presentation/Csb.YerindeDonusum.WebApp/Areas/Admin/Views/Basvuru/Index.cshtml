        function BasvuruIptalEtModal(basvuruGuid, tcKimlikNo) {
            $('#btnBasvuruIptalConfirm').attr("data-confirm-param",basvuruGuid);
            $('#btnBasvuruIptalConfirm').attr("data-confirm-param2", tcKimlikNo);
            $('#modal_basvuru_iptal').modal("show");
