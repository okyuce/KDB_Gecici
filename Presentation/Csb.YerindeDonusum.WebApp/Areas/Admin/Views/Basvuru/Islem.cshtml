                        <div class="tab-pane fade" id="kt_vtab_pane_2" role="tabpanel">
                            @if (!string.IsNullOrWhiteSpace(hasarTespitBilgilendirmeMesaji))
                            {
                                <div class="alert alert-dark mb-10">
                                    @Html.Raw(BBCode.ToHtml(hasarTespitBilgilendirmeMesaji))
                                </div>
                            }
