        <div id="container-basvuru" class="card-body p-8">
            @if (!FluentValidationExtension.NotEmpty(Model?.Id))
            {
                <div class="col-md-3 d-none">
                    <select name="BasvuruTipi"
                            data-value="1"
                            class="form-select select-2">
                        <option value="1">Gerçek Kişi</option>
                        <option value="2">Tüzel Kişi</option>
                    </select>
                </div>
            }
            <ul class="nav nav-tabs nav-line-tabs nav-line-tabs-2x mb-5 fs-6">
                @if (!FluentValidationExtension.NotEmpty(Model?.Id))
                {
                    <li id="li_tuzel" class="nav-item me-0 d-none" role="presentation">
                        <a class="btn btn-sm btn-flex btn-active-light-primary" data-bs-toggle="tab" href="#kt_vtab_pane_0" aria-selected="true" role="tab">
                            <span class="d-flex flex-column align-items-start">
                                <span class="fs-5 fw-bold">1. Firma Bilgileri</span>
                            </span>
                        </a>
                    </li>
                    <li id="li_gercek" class="nav-item me-0" role="presentation">
                        <a class="btn btn-sm btn-flex btn-active-light-primary active" data-bs-toggle="tab" href="#kt_vtab_pane_1" aria-selected="true" role="tab">
                            <span class="d-flex flex-column align-items-start">
