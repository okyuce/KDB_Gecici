                                    <div class="alert alert-primary mt-8">
                                        <div class="row">
                                            <div class="col-md-6 d-flex flex-column">
                                                <label class="d-flex align-items-center fs-6 fw-semibold mb-2">
                                                    <span>Ad</span>
                                                </label>
                                                <label id="lb_ad"></label>
                                            </div>
                                            <div class="col-md-6 d-flex flex-column">
                                                <label class="d-flex align-items-center fs-6 fw-semibold mb-2">
                                                    <span>Soyad</span>
                                                </label>
                                                <label id="lb_soyad"></label>
                                            </div>
                                        </div>
                                    </div>
