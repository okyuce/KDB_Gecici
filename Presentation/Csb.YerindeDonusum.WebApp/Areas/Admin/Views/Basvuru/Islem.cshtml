        function BasvuruEkle() {
            if (!validateForm("#basvuruForm")) {
                return;
            }
            debugger;
            // Virgülle ayırarak sınırsız obje gönderebiliriz, formData ile bu objeleri assign eder, yani formData' ya ekler.
            // örn: obj["basvuruDosyaListe"] = {};
            // obj gönderirsem formData' ya basvuruDosyaListe keyini ekler.
            let formData = serializeFormAsObject("#basvuruForm", BasvuruTapuBelgesiListe, BasvuruMuhtarBelge, BasvuruTapuBilgiListe, BasvuruTuzelYetkiDosyaListe);

            if (formData.tapuBeyan == 2 || formData.tapuBeyan == 3) {
                let tapuIlAdi = $("select[name='TapuBeyanIlId']").select2('data') ?? false;
                let tapuIlceAdi = $("select[name='TapuBeyanIlId']").select2('data') ?? false;
                let tapuMahalleAdi = $("select[name='TapuBeyanIlId']").select2('data') ?? false;
                formData["tapuIlAdi"] = tapuIlAdi ? tapuIlAdi[0]?.text?.trim() : "";
                formData["tapuIlceAdi"] = tapuIlceAdi ? tapuIlceAdi[0]?.text?.trim() : "";
                formData["tapuMahalleAdi"] = tapuMahalleAdi ? tapuMahalleAdi[0]?.text?.trim() : "";

                formData["tapuIlId"] = formData["tapuBeyanIlId"];
                formData["tapuIlceId"] = formData["tapuBeyanIlceId"];
                formData["tapuMahalleId"] = formData["tapuBeyanMahalleId"];

                formData["tapuAda"] = formData["tapuBeyanAda"];
                formData["tapuParsel"] = formData["tapuBeyanParsel"];
                formData["tapuBagimsizBolumNo"] = formData["tapuBeyanBagimsizBolumNo"];
                formData["tapuKat"] = formData["tapuBeyanKat"];
                formData["tapuBlok"] = formData["tapuBeyanBlok"];
                formData["tapuGirisBilgisi"] = formData["tapuBeyanGirisBilgisi"];
                formData["tapuNitelik"] = formData["tapuBeyanNitelik"];

                if (formData.tapuBeyan == 3) {
                    formData["tapuHazineArazisiMi"] = "true";
                }
            }

            if ($("input:checkbox[name='uavtBeyan']")[0].checked) {
                formData["uavtBeyanMi"] = "true";
                formData["uavtIcKapiNo"] = formData["uavtIcKapiNoBeyan"];
                formData["uavtDisKapiNo"] = formData["uavtDisKapiNoBeyan"];
                formData["uavtMeskenBinaNo"] = "";
            } else {
                formData["uavtBeyanMi"] = "false";
                formData["uavtDisKapiNo"] = $("select[name=UavtMeskenBinaNo]").select2('data')[0].text;
                formData["uavtIcKapiNo"] = $("select[name=UavtAdresNo]").select2('data')[0].text;
                formData["uavtAdresNo"] = $("select[name=UavtAdresNo]").select2('data')[0].id;
            }

            formData["basvuruDosyaListe"] = [];
            if (!IsNullOrEmpty(formData["basvuruTapuBelgesiListe"])) {
                formData["basvuruTapuBelgesiListe"].forEach((element) =>
                    formData["basvuruDosyaListe"].push(element)
                );
            }

            if (!IsNullOrEmpty(formData["basvuruMuhtarBelge"])) {
                formData["basvuruMuhtarBelge"].forEach((element) =>
                    formData["basvuruDosyaListe"].push(element)
                );
            }

            let basvuruTipi = $("#cbTuzelKisiBasvuru").is(':checked') === true ? 1 : 0;

            startLoader("#container-basvuru");
            Request("/admin/basvuru/" + (basvuruTipi == 1 ? "EkleTuzelKisiBasvuru" : "EkleGercekKisiBasvuru"), formData, 'post')
                .done(function (response) {
                    stopLoader("#container-basvuru");

                    localStorage.setItem("sonBasvuruGuid", response.basvuruId);
                    localStorage.setItem("sonBasvuruTc", formData.tcKimlikNo);

                    toastr.success("Başvuru başarıyla alındı. Detay sayfasına yönlendiriliyorsunuz.");
                    setTimeout(function () {
                        window.location.href = "/admin/basvuru";
                    }, 1500);
                })
                .fail(function (jqXHR, textStatus) {
                    stopLoader("#container-basvuru");
                    toastr.error(getAjaxErrorMessage(jqXHR));
                });
        }

        function BasvuruGuncelle() {
            if (!validateForm("#basvuruForm")) {
                return;
            }
            debugger;
            let formData = serializeFormAsObject("#basvuruForm", BasvuruMuhtarBelge);

            if (formData.tapuBeyan == 3) {
                formData["tapuHazineArazisiMi"] = "true";
            } else if (formData.tapuBeyan == 2) {
                formData["tapuHazineArazisiMi"] = "false";
            }

            if ($("input:checkbox[name='uavtBeyan']")[0].checked) {
                formData["uavtBeyanMi"] = "true";
                formData["uavtIcKapiNo"] = formData["uavtIcKapiNoBeyan"];
                formData["uavtDisKapiNo"] = formData["uavtDisKapiNoBeyan"];
                formData["uavtMeskenBinaNo"] = "";
            } else {
                formData["uavtBeyanMi"] = "false";
                formData["uavtDisKapiNo"] = $("select[name=UavtMeskenBinaNo]").select2('data')[0].text;
                formData["uavtIcKapiNo"] = $("select[name=UavtAdresNo]").select2('data')[0].text;
                formData["uavtAdresNo"] = $("select[name=UavtAdresNo]").select2('data')[0].id;
            }
            formData["basvuruDosyaListe"] = formData["basvuruMuhtarBelge"];

            startLoader("#container-basvuru");
            Request("/admin/basvuru/GuncelleBasvuru", formData, 'post')
                .done(function (response) {
                    stopLoader("#container-basvuru");

                    localStorage.setItem("sonBasvuruGuid", response.basvuruGuid);
                    localStorage.setItem("sonBasvuruTc", formData.tcKimlikNo);

                    toastr.success("Başvuru başarıyla güncellendi. Detay sayfasına yönlendiriliyorsunuz.");
                    setTimeout(function () {
                        window.location.href = "/admin/basvuru";
                    }, 1500);
                })
                .fail(function (jqXHR, textStatus) {
                    stopLoader("#container-basvuru");
                    toastr.error(getAjaxErrorMessage(jqXHR));
                });
        }

        function ajaxTableComplete(data, tableElem) {
            let tableName = tableElem.attr("data-name");
            if (tableName == "tableTapuBilgileri") {
                $.each(data, function (index, row) {
                    // Bağımsız Bölüm Numarası / Kat / Blok / Giriş
                    row.bagimsizBolum = (row.bagimsizBolum?.no ?? "") + " / " + (row.bagimsizBolum?.kat ?? "") + (row.bagimsizBolum?.blok ?? "") + (row.bagimsizBolum?.giris ?? "");
                    let hisseSonuc = "";
                    for (let i = 0; i < row.hisseListe?.length; i++) {
                        hisseSonuc += row.hisseListe[i].pay + " / " + row.hisseListe[i].payda + "\r\n";
                    }
                    row.hissePayPayda = hisseSonuc;
                });
            }
            return data;
        }
        function uavtBeyanMaksDegistir(data) {
            if (data) {
                $("#binaNoBeyan").show();
                $("#icNoBeyan").show();
                $("#beyanMuhtarBelgeYukleme").show();
                $("#binaNoMaks").hide();
                $("#icNoMaks").hide();
                $("#binaNoBeyan, #icNoBeyan, #beyanMuhtarBelgeYukleme").find("input").attr("data-validate", "required");
                //$("#beyanMuhtarBelgeYukleme").find("file").attr("data-validate", "required");
                $("#binaNoMaks, #icNoMaks").find("select").removeAttr("data-validate", "required");
            }
            else {
                $("#binaNoBeyan").hide();
                $("#icNoBeyan").hide();
                $("#beyanMuhtarBelgeYukleme").hide();
                $("#binaNoMaks").show();
                $("#icNoMaks").show();
                $("#binaNoMaks, #icNoMaks").find("select").attr("data-validate", "required")
                $("#binaNoBeyan, #icNoBeyan, #beyanMuhtarBelgeYukleme").find("input").removeAttr("data-validate", "required")
                //$("#beyanMuhtarBelgeYukleme").find("file").removeAttr("data-validate", "required")
            }
        }
    </script>
